function angles{
        
      [CmdletBinding()]
      param(

     [Parameter(Mandatory)]
     [int]$f,

     [Parameter(Mandatory)]
     [int]$g
     )
     $anser2 = $f + $g
     $bob = 180 - $anser2
     Write-Host " missing angle from $f and $g = $bob"
     }
     
     
