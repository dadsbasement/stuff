 function get-multisum {
        [CmdletBinding()]
        param(

        [Parameter(Mandatory)]
        [array]$listy,

        [Parameter(Mandatory)]
        [int]$singlenumber
      )
      Begin {
              $sum=0
               }
      process{
        foreach($num in $array) {
                if($num -eq $singlenumber){
                continue
                }
                  $sum += $num
            }
       }
     }  
       End {
                $sum
                }
