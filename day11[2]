function triangle{
        
      [CmdletBinding()]
      param(

     [Parameter(Mandatory)]
     [int]$a,

     [Parameter(Mandatory)]
     [int]$b

     )
     $aa = $a * $a
     $bb = $b * $b
     $c = $aa + $bb
     $answer = [math]::Sqrt($c)
     Write-Host " √$aa + √$bb = $answer "
}

